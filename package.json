{
  "name": "zyy-v3-ui",
  "version": "2.0.10",
  "description": "A Component Library for Vue 3",
  "workspaces": [
    "packages/**/*"
  ],
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js",
    "./*": "./*"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "style": "dist/index.css",
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "scripts": {
    "dev": "vite serve ./examples --config vite.config.ts",
    "dev:docs": "vitepress dev docs",
    "build:docs": "vitepress build docs",
    "serve:docs": "vitepress serve docs",
    "clean": "pnpm run clean:dist && pnpm run clean -r --stream",
    "clean:dist": "rimraf dist",
    "gen": "bash ./scripts/gc.sh",
    "update:version": "sucrase-node scripts/update-version.ts",
    "build": "gulp --require sucrase/register/ts -f build/gulpfile.ts",
    "build:theme": "pnpm run build -C packages/theme-chalk",
    "pub": "sh scripts/publish.sh",
    "build:resolver": "pnpm run build -C resolver",
    "prepare": "husky install",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx --max-warnings 0 && pretty-quick",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx && pretty-quick",
    "format": "prettier --write .",
    "preinstall": "npx only-allow pnpm"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.4",
    "dayjs": "^1.10.7",
    "element-plus": "^1.2.0-beta.6",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.16.4",
    "@babel/traverse": "^7.16.3",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@pnpm/find-workspace-packages": "^3.1.28",
    "@pnpm/logger": "^4.0.0",
    "@pnpm/types": "^7.6.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/gulp": "^4.0.9",
    "@types/lodash": "^4.14.178",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.3.2",
    "@vue/compiler-sfc": "^3.2.26",
    "@vueuse/core": "^7.3.0",
    "@zyy-v3-ui/components": "./packages/components",
    "@zyy-v3-ui/theme-chalk": "./packages/theme-chalk",
    "chalk": "4.1.2",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-define-config": "^1.2.5",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "fast-glob": "^3.2.7",
    "filesize": "^8.0.6",
    "fs-extra": "^10.0.0",
    "gulp": "^4.0.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.4",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "rimraf": "^3.0.2",
    "rollup": "2.61.1",
    "rollup-plugin-css-only": "3.1.0",
    "rollup-plugin-esbuild": "^4.7.2",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^6.0.0",
    "sass": "^1.44.0",
    "select-version-cli": "^0.0.2",
    "sucrase": "^3.20.3",
    "ts-morph": "^13.0.2",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "unplugin-auto-import": "^0.5.3",
    "unplugin-vue-components": "^0.17.9",
    "vite": "^2.6.4",
    "vitepress": "^0.20.1",
    "vitepress-theme-demoblock": "^1.4.2",
    "vue": "^3.2.0"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx}": "eslint --fix"
  }
}
